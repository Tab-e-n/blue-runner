script/source = "extends Control

onready var level_name : LineEdit = $GUI/File/Panel/name_edit
onready var creator : LineEdit = $GUI/File/Panel/creator_edit
onready var official : CheckBox = $GUI/File/Panel/official_edit
onready var level_icon : OptionButton = $GUI/File/Panel/icon_edit
var dependencies : Array
onready var tilesets : OptionButton = $GUI/File/Panel/tileset_edit
onready var bg : OptionButton = $GUI/File/Panel/bg_edit

onready var place_tiles : TileMap = $GUI/Place/Panel/tab_tiles
var place_decor : Dictionary

var place_tiles_items : Array
var place_objects_items : Array
var place_decor_items : Array
const PLACE_ITEM_AMOUNT : int = 16

onready var items_scroll : VScrollBar = $GUI/Items/Panel/scroll
onready var items_decor_directory : OptionButton = $GUI/Items/Panel/decor_directory

var mods_installed : Array

enum {PLACEMODE_MOVE, PLACEMODE_PAINT, PLACEMODE_ERASER, PLACEMODE_COG}
var place_current_mode : int = PLACEMODE_MOVE

enum {PLACE_TILES, PLACE_OBJECTS, PLACE_DECOR}
var place_current_tab : int = PLACE_TILES

var user_directory : String = \"user://sonicRunnerEditor\"
# Max 8 faves per category
# Each tileset has its own 8 faves under its name
var favorite : Dictionary = {
	\"Objects\" : [],
	\"Decor\" : [],
}
# Mirror recent in style, however with 16 instead of 8 things
var recent : Dictionary = {
	\"Objects\" : [],
	\"Decor\" : [],
}

# Here lies what will be saved in the .dat file of levels,
# Delete once you have the .dat save code ready
var level_data : Dictionary = {
	\"level_name\" : \"[name]\",
	\"creator\" : \"Tabin\",
	\"official\" : true,
	\"level_icon\" : [\"questionmark\",\"res:/\"],
	\"dependencies\" : [],
}

func _ready():
	load_game()
	load_editor_prop()
	
	tileset_changed(tilesets.get_item_index(tilesets.get_selected_id()))
	
	update_object_items()
	
	for i in place_decor.keys():
		items_decor_directory.add_item(i)
	update_itemspanel_decor(0, false)
	update_decor_items()
	
	_on_tab_move()

func _process(_delta):
	var window = get_viewport().get_visible_rect().end
	$GUI.margin_right = window.x / 2
	$GUI.margin_left = window.x / -2
	$GUI.margin_bottom = window.y / 2
	$GUI.margin_top = window.y / -2

func _exit_tree():
	save_game()

func save_game():
	
	var savefile = File.new()
	var temp = {}
	
	temp[\"favorite\"] = favorite.duplicate()
	temp[\"recent\"] = recent.duplicate()
	
	savefile.open(user_directory, File.WRITE)
	savefile.store_line(to_json(temp))
	savefile.close()

func load_game(fail = true):
	
	var loadfile = File.new()
	var temp = {}
	
	if not loadfile.file_exists(user_directory) or fail: # does file exist
		save_game()
		return
	
	loadfile.open(user_directory, File.READ)
	
	while loadfile.get_position() < loadfile.get_len():
		var parsedData = parse_json(loadfile.get_line())
		
		temp = parsedData
	
	loadfile.close()
	
	#print(\"load: \", temp)
	
	favorite = temp[\"favorite\"].duplicate()
	recent = temp[\"recent\"].duplicate()

func load_editor_prop():
	# This section of code loads the names of certain objects,
	# Their actual data will be loaded once needed
	var directory : Directory = Directory.new()
	var current_file : String
	
	# VANILLA
	
	print(\"scanning vanilla tilesets\")
	directory.open(\"res://Objects/Tilesets\")
	directory.list_dir_begin(true)
	
	current_file = directory.get_next()
	while current_file != \"\":
		print(\"found tileset: \" + current_file)
		tilesets.add_item(current_file)
		tilesets.set_item_metadata(tilesets.get_item_count() - 1, \"res:/\")
		current_file = directory.get_next()
	
	# BG
	print(\"scanning vanilla backgrounds\")
	directory.open(\"res://Objects/Backgrounds\")
	directory.list_dir_begin(true)
	
	current_file = directory.get_next()
	while current_file != \"\":
		print(\"found background: \" + current_file)
		bg.add_item(current_file)
		bg.set_item_metadata(bg.get_item_count() - 1, \"res:/\")
		current_file = directory.get_next()
	
	# Other Objects
	# Decor
	print(\"scanning vanilla decor\")
	place_decor[\"res://Visual/\"] = []
	var directories_to_visit : Array = [\"res://Visual/\"]
	
	while directories_to_visit.size() > 0:
		var current_directory : String = directories_to_visit[0]
		print(\"looking through: \" + current_directory)
		directory.open(current_directory)
		directory.list_dir_begin(true)
		current_file = directory.get_next()
		while current_file != \"\":
			if directory.current_is_dir():
				print(\"found folder: \" + current_file)
				place_decor[current_directory + current_file + \"/\"] = []
				directories_to_visit.append(current_directory + current_file + \"/\")
			elif current_file.ends_with(\".png\"):
				#print(\"found decor: \" + current_file)
				place_decor[current_directory].append(current_directory + current_file)
			current_file = directory.get_next()
		directories_to_visit.pop_front()
	
	# MODS
	
	print(\"scanning mods\")
	var check_exist = directory.open(\"Mods\")
	
	if check_exist == OK:
		print(\"user has mods installed\")
		
		directory.list_dir_begin(true)
		current_file = directory.get_next()
		while current_file != \"\":
			mods_installed.append(current_file)
			current_file = directory.get_next()
	else:
		print(\"no mods found, or an error occured\")
	
	for mod_name in mods_installed:
		print(\"found mod: \" + mod_name)
		directory.open(\"Mods/\" + mod_name)
		
		# Tilesets
		print(\"scanning for tilesets\")
		check_exist = directory.open(\"Mods/\" + mod_name + \"/Objects/Tilesets\")
		
		if check_exist == OK:
			tilesets.add_item(\" - \" + mod_name + \" - \")
			tilesets.set_item_disabled(tilesets.get_item_count() - 1, true)
			directory.list_dir_begin(true)
			
			current_file = directory.get_next()
			while current_file != \"\":
				print(\"found tileset: \" + current_file)
				tilesets.add_item(current_file)
				tilesets.set_item_metadata(tilesets.get_item_count() - 1, \"Mods/\" + mod_name)
				current_file = directory.get_next()
		
		# BG
		print(\"scanning for backgrounds\")
		check_exist = directory.open(\"Mods/\" + mod_name + \"/Objects/Backgrounds\")
		
		if check_exist == OK:
			bg.add_item(\" - \" + mod_name + \" - \")
			bg.set_item_disabled(bg.get_item_count() - 1, true)
			directory.list_dir_begin(true)
			
			current_file = directory.get_next()
			while current_file != \"\":
				print(\"found background: \" + current_file)
				bg.add_item(current_file)
				bg.set_item_metadata(bg.get_item_count() - 1, \"Mods/\" + mod_name)
				current_file = directory.get_next()
		
		# Other Objects
		# Visuals
		print(\"scanning for decor\")
		check_exist = directory.open(\"Mods/\" + mod_name + \"/Visual/\")
		if check_exist == OK:
			place_decor[\"Mods/\" + mod_name + \"/Visual/\"] = []
			directories_to_visit = [\"Mods/\" + mod_name + \"/Visual/\"]
		
		while directories_to_visit.size() > 0:
			var current_directory : String = directories_to_visit[0]
			print(\"looking through: \" + current_directory)
			directory.open(current_directory)
			directory.list_dir_begin(true)
			current_file = directory.get_next()
			while current_file != \"\":
				if directory.current_is_dir():
					print(\"found folder: \" + current_file)
					place_decor[current_directory + current_file + \"/\"] = []
					directories_to_visit.append(current_directory + current_file + \"/\")
				elif !current_file.ends_with(\".import\"):
					#print(\"found decor: \" + current_file)
					place_decor[current_directory].append(current_directory + current_file)
				current_file = directory.get_next()
			directories_to_visit.pop_front()

