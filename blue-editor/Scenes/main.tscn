	#TBA

func tileset_changed(index):
	var tileset_path : String = tilesets.get_selected_metadata() + \"/Objects/Tilesets/\" + tilesets.get_item_text(index)
	place_tiles.tile_set = load(tileset_path)
	update_tile_items(tilesets.get_item_text(index))

# PLACE FUNCTIONS
func update_tile_items(tileset_name : String):
	if favorite.has(tileset_name):
		place_tiles_items = favorite[tileset_name].duplicate()
	if recent.has(tileset_name):
		for i in range(recent[tileset_name].size() - place_tiles_items.size()):
			place_tiles_items.append(recent[tileset_name][i])
	for _i in range(PLACE_ITEM_AMOUNT - place_tiles_items.size()):
		place_tiles_items.append(-1)
	
	for x in range(PLACE_ITEM_AMOUNT):
		var y : int = 0
		if x > 7: y = 1
		place_tiles.set_cell(x - y * 8, 1 - y, place_tiles_items[x])
func update_object_items():
	place_objects_items = favorite[\"Objects\"].duplicate()
	for i in range(recent[\"Objects\"].size() - place_objects_items.size()):
		place_objects_items.append(recent[\"Objects\"][i])
	for _i in range(PLACE_ITEM_AMOUNT - place_objects_items.size()):
		place_objects_items.append(-1)
	# TBA
func update_decor_items():
	place_decor_items = favorite[\"Decor\"].duplicate()
	for i in range(recent[\"Decor\"].size() - place_decor_items.size()):
		place_decor_items.append(recent[\"Decor\"][i])
	for _i in range(PLACE_ITEM_AMOUNT - place_decor_items.size()):
		place_decor_items.append(\"\")
	
	for i in range(PLACE_ITEM_AMOUNT):
		var current_sprite : Sprite = get_node(\"GUI/Place/Panel/tab_decor/\" + String(i))
		
		if place_decor_items[i] == \"\":
			current_sprite.texture = load(\"res://Visual/Editor/editor_empty.png\")
		else:
			var loaded_texture : Texture = load(place_decor_items[i])
			if loaded_texture == null:
				current_sprite.texture = load(\"res://Visual/Editor/editor_missing.png\")
			else:
				current_sprite.texture = loaded_texture
			scale_down_sprite(current_sprite)

func scale_down_sprite(item : Sprite):
	item.scale = Vector2(1, 1)
	var item_rect : Vector2 = item.get_rect().size
	if item_rect.x > item_rect.y:
		item.scale.x = 1 / (item_rect.x / 64)
	else:
		item.scale.x = 1 / (item_rect.y / 64)
	item.scale.y = item.scale.x

func _on_mode_toggled(button_pressed, mode):
	if button_pressed:
		place_current_mode = mode
		if !mode == PLACEMODE_PAINT:
			$GUI/Place/Panel/paint.pressed = false
		if !mode == PLACEMODE_ERASER:
			$GUI/Place/Panel/eraser.pressed = false
		if !mode == PLACEMODE_COG:
			$GUI/Place/Panel/cog.pressed = false
	else:
		place_current_mode = PLACEMODE_MOVE

func _on_tab_move(direction : int = 0):
	place_current_tab += direction
	if place_current_tab < PLACE_TILES:
		place_current_tab = PLACE_DECOR
	if place_current_tab > PLACE_DECOR:
		place_current_tab = PLACE_TILES
	
	place_tiles.visible = false
	$GUI/Place/Panel/tab_decor.visible = false
	$GUI/Items/Panel/items.visible = true
	$GUI/Items/Panel/decor_directory.visible = false
	$GUI/Items/Panel/objects_directory.visible = false
	match(place_current_tab):
		PLACE_TILES:
			$GUI/Place/Panel/tab_text.text = \"Tiles\"
			place_tiles.visible = true
			$GUI/Items/Panel/items.visible = false
		PLACE_OBJECTS:
			$GUI/Place/Panel/tab_text.text = \"Objects\"
			$GUI/Items/Panel/objects_directory.visible = true
		PLACE_DECOR:
			$GUI/Place/Panel/tab_text.text = \"Decor\"
			$GUI/Place/Panel/tab_decor.visible = true
			$GUI/Items/Panel/decor_directory.visible = true
			change_items_decor(items_scroll.value, items_decor_directory.get_item_text(items_decor_directory.selected))

func _place_item_selected(index):
	match(place_current_tab):
		PLACE_TILES:
			$GUI/Place/Panel/tab_tiles/temp_selected_tile.text = String(index)
			place_tiles.set_cell(-1, 0, place_tiles_items[index])
		PLACE_OBJECTS:
			prepare_item_for_placing()
		PLACE_DECOR:
			prepare_item_for_placing()

func prepare_item_for_placing():
	pass
	#TBA

# ITEMS FUNC
func update_itemspanel_decor(index : int, change_items : bool = true):
	var selected_directory : String = items_decor_directory.get_item_text(index)
	# warning-ignore:integer_division
	var row_amount : int = int(place_decor[selected_directory].size()) / 7 + 1
	if row_amount > 7:
		items_scroll.max_value = row_amount - 6
	else:
		items_scroll.max_value = 1
	items_scroll.value = 0
	
	if change_items: change_items_decor(items_scroll.value, selected_directory)

func change_items_decor(current_row, directory):
	for x in range(7): for y in range(7):
		var sprite_index : int = x+(y+current_row)*7
		var loaded_texture : Texture
		var sprite = get_node(\"GUI/Items/Panel/items/\" + String(x) + String(y))
		if sprite_index < place_decor[directory].size():
			loaded_texture = load(place_decor[directory][sprite_index])
		else:
			loaded_texture = load(\"res://Visual/Editor/editor_empty.png\")
		sprite.texture = loaded_texture
		scale_down_sprite(sprite)

func _on_items_scroll_scrolling():
	change_items_decor(items_scroll.value, items_decor_directory.get_item_text(items_decor_directory.selected))
